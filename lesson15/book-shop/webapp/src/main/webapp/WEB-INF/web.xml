<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>TestWebApp</display-name>

	<servlet>
		<servlet-name>Book</servlet-name>
		<servlet-class>com.senla.training.kononovich.webapp.BookServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Claim</servlet-name>
		<servlet-class>com.senla.training.kononovich.webapp.ClaimServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Order</servlet-name>
		<servlet-class>com.senla.training.kononovich.webapp.OrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Book</servlet-name>
		<url-pattern>/book</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Claim</servlet-name>
		<url-pattern>/claim</url-pattern>
	</servlet-mapping>
		<servlet-mapping>
		<servlet-name>Order</servlet-name>
		<url-pattern>/order</url-pattern>
	</servlet-mapping>

</web-app>